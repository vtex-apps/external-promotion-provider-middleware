{
  "memory": 256,
  "ttl": 10,
  "timeout": 30,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "orderFormNotification": {
      "path": "/_v/promotion-provider/notification",
      "public": true
    },
    "simulation": {
      "path": "/_v/promotion-provider/simulation",
      "public": true
    }
  },
  "events": {
    "onAppInstalled": {
      "sender": "apps",
      "keys": ["setup"],
      "settingsType": "workspace"
    },
    "onSettingsChanged": {
      "sender": "apps",
      "keys": ["settings"]
    }
  }
}
